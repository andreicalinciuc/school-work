#include <iostream>

using namespace std;

int v[101], f, k, n, i, j, a[101], m;

int main()
{
    cin >> n;
    for (i=1; i<=n; i++)
    {
        cin >> v[i];
        if (v[i]%2==1&&!f) {f=1; m=i;}
    }
    i=1; j=m;
    while (i<=m-1&&j<=n)
    {
        if (v[i]<v[j]) a[++k]=v[i++];
        else a[++k]=v[j++];
    }
    while (i<=m-1) a[++k]=v[i++];
    while (j<=n) a[++k]=v[j++];
    for (i=1; i<=n; i++) cout << a[i] << " ";
}
